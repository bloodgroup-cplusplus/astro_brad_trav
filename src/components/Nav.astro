---
// library imports
import {Icon} from "astro-icon"
import logo from "/logo.webp"

// data imports 
import navData from "../data/navData"

// link imports 
import Link from "./Link.astro";
---
<nav aria-label="Primary">
<div class="navbar">
    <a href="/" class="logomark" aria-label="Go home">
        <Icon src={logo}  width="64"/>
    </a>
    <ul>
        {
            navData.map((item)=>(
                <Link
                  text={item.name}
                  href={item.path}
                  style="primary"
                  isFilled={false}
                  data-navlink
                  
                  />

            ))
        }


    </ul>
</div>

</nav>

<script>
    import "../js/nav.js"
</script>